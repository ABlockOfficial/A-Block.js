{
  "name": "zenotta-js",
  "version": "0.0.1",
  "description": "API module access the Zenotta network",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "module": "lib/index.es.js",
  "browser": "lib/index.js",
  "source": "src/index.ts",
  "scripts": {
    "build": "microbundle",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --config jest.config.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "ZNP",
    "Zenotta",
    "API Module",
    "typescript"
  ],
  "author": "Anton Troskie",
  "license": "MIT",
  "devDependencies": {
    "@types/bitcore-lib": "^0.15.3",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "eslint": "7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-neverthrow": "^1.1.4",
    "eslint-plugin-typesafe": "^0.5.2",
    "jest": "27.4.7",
    "microbundle": "^0.14.2",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "tslib": "^2.1.0",
    "typescript": "4.5.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.25.0",
    "bitcore-lib": "^8.25.25",
    "bitcore-mnemonic": "^8.25.25",
    "byte-base64": "^1.1.0",
    "js-sha3": "^0.8.0",
    "neverthrow": "^4.3.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.2"
  },
  "bundledDependencies": [
    "axios",
    "bitcore-lib",
    "bitcore-mnemonic",
    "byte-base64",
    "js-sha3",
    "neverthrow",
    "tweetnacl",
    "uuid"
  ]
}